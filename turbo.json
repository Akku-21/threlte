{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "package": {
			"outputs": ["package/*"],
			"cache": false
		},
    "pack": {
      "dependsOn": ["package"],
			"cache": false
		},
    "publish": {
			"dependsOn": ["^package"],
			"outputs": [],
			"cache": false
		},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "preview": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
